  <Code>'************************************************************
' Used to divide numbers in an expression and avoid dividing by 0
'************************************************************
Function SafeDivide(ByVal numerator As Decimal, ByVal denominator As Decimal) As Decimal
        If (denominator = 0) Then
            Return 0
        Else
            Return numerator / denominator
        End If
 End Function

'************************************************************
' Format report heading period textbox for Audit reports
'************************************************************
Function FormatAuditDateRange(ByVal startDateStr As String, ByVal endDateStr As String) As String
	if startDateStr = "" and endDateStr = "" then
		Return ""
	end if
	if startDateStr = "" then
		Return String.Format("[LabelThroughDate0]", endDateStr)
	else
		if endDateStr = "" then
			Return String.Format("[Label0AndAfter]", startDateStr)
		else
			Return String.Format("[Label01]", startDateStr, endDateStr)
		end if
	end if
End Function</Code>
