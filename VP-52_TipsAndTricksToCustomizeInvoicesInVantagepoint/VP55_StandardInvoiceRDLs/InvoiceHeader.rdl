  <Code>
    public function dltkSafeDivide(byVal numerator as Decimal, byVal denominator as Decimal) as Decimal
    if (denominator = 0) then
    return 0
    else
    return numerator / denominator
    end if
    end function

    public function LeadingZeros(byVal Invoice as String) as String
    Dim i As Integer
    Dim wkstr As String
    i = 1
    wkstr =Invoice
    Do While Mid$(wkstr, i, 1) = "0"
    i = i + 1
    Loop
    wkstr = Mid$(wkstr, i)
    LeadingZeros = wkstr
    end function

    Public Function FormatDateForCulture(ByVal wkCultureName As String) As String
    Dim curCulture As New System.Globalization.CultureInfo(wkCultureName)
    Dim dtPattern as String
    If curCulture.DateTimeFormat.MonthDayPattern.ToLower.StartsWith("m") Then
    dtPattern = "MMMM dd, yyyy"
    Else
    dtPattern = "dd MMMM yyyy"
    End If
    Return dtPattern
    End Function

