    <DataSet Name="ReportDataset">
      <Query>
        <DataSourceName>VisionDemo50</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PrintAuthPrefix">
            <Value>=Parameters!PrintAuthPrefix.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PrintAuthSuffix">
            <Value>=Parameters!PrintAuthSuffix.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ActiveCompany">
            <Value>=Parameters!ActiveCompany.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmMainWBS1">
            <Value>=Parameters!parmMainWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmInvoice">
            <Value>=Parameters!parmInvoice.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT distinct
billInvMaster.Invoice, 
billInvMaster.MainWBS1, 
billInvMaster.InvoiceDate, 
billInvMaster.MainName, 
billInvMaster.MainType, 
billInvMaster.BillGroup, 
billInvMaster.BillName, 
billInvMaster.Addressee, 
billInvMaster.Address1, 
billInvMaster.Address2, 
billInvMaster.Address3, 
billInvMaster.Address4, 
billInvMaster.Address5, 
billInvMaster.Address6, 
billInvMaster.Address7, 
billInvMaster.Address8, 
billInvMaster.Address9, 
billInvMaster.Address10, 
billInvMaster.TaxCountryCode, 
billInvMaster.DueDate,
case when CFGMainData.TaxAuditingEnabled='Y' then billInvMaster.TaxRegistrationNumber else '' end As ClientTaxReg, 
case when CFGMainData.TaxAuditingEnabled='Y' then CFGMainData.TaxRegistrationNumber else '' end as FirmTaxReg, 
CFGMainData.firmname As FirmName, 
CFGMainData.byline As FirmByLine, 
CFGMainData.address1 As FirmAddress1, 
CFGMainData.address2 As FirmAddress2, 
CFGMainData.address3 As FirmAddress3, 
CFGMainData.address4 As FirmAddress4, 
CASE WHEN @PrintAuthPrefix = 'Y' THEN ISNULL(EMProjMgr.Salutation+' ','') ELSE '' END +
EMProjMgr.FirstName + ' ' + EMProjMgr.LastName  +
CASE WHEN @PrintAuthSuffix = 'Y' THEN ' ' + ISNULL(EMProjMgr.Suffix,'') ELSE '' END As ProjMgrName, 
CASE WHEN @PrintAuthPrefix = 'Y' THEN ISNULL(EMPrincipal.Salutation+' ','') ELSE '' END +
EMPrincipal.FirstName + ' ' + EMPrincipal.LastName  +
CASE WHEN @PrintAuthSuffix = 'Y' THEN ' ' + ISNULL(EMPrincipal.Suffix,'') ELSE '' END As PrincipalName, 
CASE WHEN @PrintAuthPrefix = 'Y' THEN ISNULL(EMSupervisor.Salutation+' ','') ELSE '' END +
EMSupervisor.FirstName + ' ' + EMSupervisor.LastName  +
CASE WHEN @PrintAuthSuffix = 'Y' THEN ' ' + ISNULL(EMSupervisor.Suffix,'') ELSE '' END As SupervisorName, 
PR.Org As Org, PR.FeeBillingCurrency As Fee, 
PR.ReimbAllowBillingCurrency As ReimbAllow, 
PR.ConsultFeeBillingCurrency As ConsultFee, 
PR.FeeBillingCurrency + PR.ReimbAllowBillingCurrency + PR.ConsultFeeBillingCurrency  As TotalComp,
PR.BillingCurrencyCode As CurrencyCode, AR.Due As NowDue
/***ADDITIONAL_COLUMNS***/
 FROM BillInvMaster 
LEFT JOIN BT ON billInvMaster.MainWBS1 = BT.WBS1 And BT.WBS2 = /*N*/' ' And BT.WBS3 = /*N*/' '
LEFT JOIN (Select MainWBS1, Invoice, Sum(Balance + Retainage) As Due From billARDetail Where (Invoice = @parmInvoice) AND (billARDetail.MainWBS1 = @parmMainWBS1) Group By MainWBS1, Invoice) As AR ON billInvMaster.MainWBS1 = AR.MainWBS1 And billInvMaster.Invoice = AR.Invoice,
PR LEFT join EM AS EMProjMgr ON PR.Projmgr = EMProjMgr.Employee  
LEFT join EM AS EMPrincipal ON PR.Principal = EMPrincipal.Employee  
LEFT join EM AS EMSupervisor ON PR.Supervisor = EMSupervisor.Employee  
LEFT Join ProjectCustomTabFields On PR.WBS1 = ProjectCustomTabFields.WBS1 And ProjectCustomTabFields.WBS2 = /*N*/' ' And ProjectCustomTabFields.WBS3 = /*N*/' '  
LEFT JOIN CL ON CL.ClientID = PR.ClientID  
LEFT JOIN CL AS CLBill ON CLBill.ClientID = PR.BillingClientID
LEFT Join Contacts On PR.BillingContactID = Contacts.ContactID 
/***ADDITIONAL_JOINS***/
,  
CFGMainData
WHERE CFGMainData.Company =@ActiveCompany
AND (billInvMaster.MainWBS1 = PR.WBS1 AND PR.WBS2 = /*N*/' ' AND PR.WBS3 = /*N*/' ')  
AND (MainType != 'S')
AND billInvMaster.MainWBS1 = @parmMainWBS1  
AND billInvMaster.Invoice = @parmInvoice</CommandText>
