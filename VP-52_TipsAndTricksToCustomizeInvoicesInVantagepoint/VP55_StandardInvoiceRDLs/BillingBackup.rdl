<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DefaultDataSource">
      <DataSourceReference>DefaultDataSource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>757cccad-314c-43e6-9f38-26619b870391</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="ReportDataset">
      <Query>
        <DataSourceName>DefaultDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ActiveCompany">
            <Value>=Parameters!ActiveCompany.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmMainWBS1">
            <Value>=Parameters!parmMainWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PreInvoice">
            <Value>=Parameters!PreInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmInvoice">
            <Value>=Parameters!parmInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmFixedFee">
            <Value>=Parameters!parmFixedFee.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmShowLabExt">
            <Value>=Parameters!parmShowLabExt.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@showBillingBackup">
            <Value>=Parameters!showBillingBackup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmShowBillingBackupLabor">
            <Value>=Parameters!parmShowBillingBackupLabor.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmShowBillingBackupExpense">
            <Value>=Parameters!parmShowBillingBackupExpense.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmShowBackupFees">
            <Value>=Parameters!BackupFees.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmShowBackupAddOns">
            <Value>=Parameters!BackupAddOns.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select max(NonBill) as NonBill, max(invoiceKey) as InvoiceKey, Invoice,
          MainWBS1, BillWBS1, BillWBS2, BillWBS3,
          max(InvoiceDate) AS InvoiceDate,
          max(MainName) AS MainName, max(MainType) AS MainType, max(BillGroup) AS BillGroup,
          max(ConsolidatePrint) AS ConsolidatePrint, max(ByGroupWBS1) AS ByGroupWBS1, max(BillName) AS BillName,
          max(ConsolidateWBS2) AS ConsolidateWBS2, max(ConsolidateWBS3) AS ConsolidateWBS3,
          max(BillWBS2Name) AS BillWBS2Name,
          max(BillWBS3Name) AS BillWBS3Name, max(FromDate) AS FromDate, max(ToDate) AS ToDate, max(WBS2Rollup) AS WBS2Rollup,
          max(WBS3Rollup) AS WBS3Rollup, max(Description) As Description,
          sum(billBack.wbs3Total) AS wbs3Total,
          sum(wbs2Section.wbs2Amount) AS wbs2Total,
          sum(wbs1Section.wbs1Amount) AS wbs1Total,
          sum(invSection.invAmount) As invTotal,
          max(LongName) As LongName,
          max(WBS2LongName) As WBS2LongName,
          max(WBS3LongName) As WBS3LongName,
          max(BillingCurrencyCode) As CurrencyCode, FirmName = (Select FirmName From CFGMainData Where Company = @ActiveCompany)         
          FROM (
          Select max(NonBill) as NonBill, max(invoiceKey) as InvoiceKey, max(Invoice) AS Invoice,
          max(MainWBS1) AS MainWBS1, max(BillWBS1) AS BillWBS1, max(BillWBS2) AS BillWBS2, max(BillWBS3) AS BillWBS3,
          max(InvoiceDate) AS InvoiceDate,
          max(MainName) AS MainName, max(MainType) AS MainType, max(BillGroup) AS BillGroup,
          max(ConsolidatePrint) AS ConsolidatePrint, max(ByGroupWBS1) AS ByGroupWBS1, max(BillName) AS BillName,
          max(ConsolidateWBS2) AS ConsolidateWBS2, max(ConsolidateWBS3) AS ConsolidateWBS3,
          max(BillWBS2Name) AS BillWBS2Name,
          max(BillWBS3Name) AS BillWBS3Name, max(FromDate) AS FromDate, max(ToDate) AS ToDate, max(WBS2Rollup) AS WBS2Rollup,
          max(WBS3Rollup) AS WBS3Rollup, max(Description) As Description,
          sum(wbs3Amount) AS wbs3Total,
          max(LongName) As LongName,
          max(WBS2LongName) As WBS2LongName,
          max(WBS3LongName) As WBS3LongName,
          max(BillingCurrencyCode) As BillingCurrencyCode          
          FROM (
          SELECT 0 AS NonBill, billInvMaster.invoice + billInvMaster.mainWBS1 As invoiceKey,
          billInvMaster.Invoice,
          billInvMaster.MainWBS1,
          billInvMaster.BillWBS1,
          billInvMaster.BillWBS2,
          billInvMaster.BillWBS3,
          billInvMaster.InvoiceDate,
          billInvMaster.MainName,
          billInvMaster.MainType,
          billInvMaster.BillGroup,
          billInvMaster.ConsolidatePrint,
          billInvMaster.ByGroupWBS1,
          billInvMaster.BillName,
          billInvMaster.ConsolidateWBS2,
          billInvMaster.ConsolidateWBS3,
          billInvMaster.BillWBS2Name,
          billInvMaster.BillWBS3Name,
          billInvMaster.FromDate,
          billInvMaster.ToDate,
          billInvMaster.WBS2Rollup,
          billInvMaster.WBS3Rollup,
          billInvMaster.Description,
          wbs3Section.wbs3Amount AS wbs3Amount,
          IsNull(PR1.LongName, PR1.Name) As LongName,
          IsNull(PR2.LongName, PR2.Name) As WBS2LongName,
          IsNull(PR3.LongName, PR3.Name) As WBS3LongName,
          PRMain.BillingCurrencyCode
          
          FROM billInvMaster left join
          PR As PRMain On PRMain.WBS1 = billInvMaster.MainWBS1 And PRMain.WBS2 = /*N*/' ' And PRMain.WBS3 = /*N*/' '
          Left Join
          PR As PR1 On PR1.WBS1 = billInvMaster.BillWBS1 And PR1.WBS2 = /*N*/' ' And PR1.WBS3 = /*N*/' '
          Left Join
          PR As PR2 On PR2.WBS1 = billInvMaster.BillWBS1 And PR2.WBS2 = billInvMaster.BillWBS2 And PR2.WBS3 = /*N*/' '
          Left Join
          PR As PR3 On PR3.WBS1 = billInvMaster.BillWBS1 And PR3.WBS2 = billInvMaster.BillWBS2 And PR3.WBS3 = billInvMaster.BillWBS3

          /*SubQuery to get wbs3 totals */
          left join (SELECT
          billInvSums.invoice as sumInvoice,
          billInvSums.mainWBS1 as sumMainWBS1,
          billInvSums.BillWBS1 as sumBillWBS1,
          billInvSums.BillWBS2 as sumBillWBS2,
          billInvSums.BillWBS3 as sumBillWBS3,
          sum(billInvSums.finalAmount) AS wbs3Amount
          FROM billInvSums
          WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' 
          And (Section In (/*N*/'L', /*N*/'E', /*N*/'C', /*N*/'U') Or (Section = /*N*/'F' And @parmShowBackupFees = 'Y') Or (Section In (/*N*/'A',/*N*/'Retainer') And @parmShowBackupAddOns = 'Y')) 
          And billInvSums.MainWBS1 = @parmMainWBS1
          AND billInvSums.Invoice = @parmInvoice AND @showBillingBackup = 'Y'
          GROUP BY billInvSums.invoice, billInvSums.mainWBS1,
          billInvSums.BillWBS1, billInvSums.BillWBS2, billInvSums.BillWBS3) AS wbs3Section
          on (billInvMaster.invoice = wbs3Section.sumInvoice) AND
          (billInvMaster.mainWBS1 = wbs3Section.sumMainWBS1) AND
          (billInvMaster.BillWBS1 = wbs3Section.sumBillWBS1) AND
          (billInvMaster.BillWBS2 = wbs3Section.sumBillWBS2) AND
          (billInvMaster.BillWBS3 = wbs3Section.sumBillWBS3)

          WHERE @PreInvoice = 'N' And billInvMaster.MainWBS1 = @parmMainWBS1
          AND billInvMaster.Invoice = @parmInvoice
          AND @showBillingBackup = 'Y'

          /*Do Fixed Fee portion of the query*/
          UNION ALL SELECT 1 AS NonBill,
          @parmInvoice + @parmMainWBS1 AS invoiceKey, @parmInvoice AS Invoice,
          max(billLab.MainWBS1) AS MainWBS1, max(BillLab.BillWBS1) AS BillWBS1, max(BillLab.BillWBS2) AS BillWBS2,
          max(BillLab.BillWBS3) AS BillWBS3, '1/1/1900' AS InvoiceDate,
          max(MainPR.Name) AS MainName, 'I' AS MainType, '' AS BillGroup, '' AS ConsolidatePrint,
          '' AS ByGroupWBS1, max(BillPR.Name) AS BillName, '' AS ConsolidateWBS2, '' AS ConsolidateWBS3,
          max(LEVEL2.Name) AS BillWBS2Name, max(LEVEL3.Name) AS BillWBS3Name,
          '1/1/1900' AS FromDate, '1/1/1900' AS ToDate, 0 AS WBS2Rollup, 0 AS WBS3Rollup, '' As Description,
          max(billLab.Extension) AS wbs3Amount,
          max(IsNull(BillPR.LongName, BillPR.Name)) As LongName,
          max(IsNull(LEVEL2.LongName, LEVEL2.Name)) As WBS2LongName,
          max(IsNull(LEVEL3.LongName, LEVEL3.Name)) As WBS3LongName,
          max(MainPR.BillingCurrencyCode) As CurrencyCode

          FROM
          PR AS BillPR,
          PR AS MainPR,
          PR AS LEVEL2,
          PR AS LEVEL3,
          (Select Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3, Sum(CASE WHEN @parmShowLabExt = 'Y' THEN billLabDetail.FullExtension ELSE 0 END) AS Extension FROM billLabDetail
          WHERE @PreInvoice = 'N' And @parmFixedFee = 'Y' AND FixedFee = 'Y' AND @showBillingBackup = 'Y' And billLabDetail.MainWBS1 = @parmMainWBS1
          AND billLabDetail.Invoice = @parmInvoice And @parmShowBillingBackupLabor = 'Y'
          GROUP BY Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3) AS billLab

          WHERE @PreInvoice = 'N' And (LEVEL3.WBS1 = billLab.BillWBS1) AND (LEVEL3.WBS2 = billLab.BillWBS2) AND (LEVEL3.WBS3 = billLab.BillWBS3)
          AND (LEVEL2.WBS1 = billLab.BillWBS1) AND (LEVEL2.WBS2 = billLab.BillWBS2) AND (LEVEL2.WBS3 = /*N*/' ')
          AND (BillPR.WBS1 = billLab.BillWBS1) AND (BillPR.WBS2 = /*N*/' ')
          AND (MainPR.WBS1 = billLab.MainWBS1) AND (MainPR.WBS2 = /*N*/' ')
          AND billLab.MainWBS1 = @parmMainWBS1 AND @showBillingBackup = 'Y' And @parmFixedFee = 'Y'
          Group By billLab.MainWBS1, billLab.BillWBS1, billLab.BillWBS2, billLab.BillWBS3

          /*Do Non Billable Expenses portion of the query*/
          UNION ALL SELECT 1 AS NonBill,
          @parmInvoice + @parmMainWBS1 AS invoiceKey, @parmInvoice AS Invoice,
          max(billExpenses.MainWBS1) AS MainWBS1, max(billExpenses.BillWBS1) AS BillWBS1, max(billExpenses.BillWBS2) AS BillWBS2,
          max(billExpenses.BillWBS3) AS BillWBS3, '1/1/1900' AS InvoiceDate,
          max(MainPR.Name) AS MainName, 'I' AS MainType, '' AS BillGroup, '' AS ConsolidatePrint,
          '' AS ByGroupWBS1, max(BillPR.Name) AS BillName, '' AS ConsolidateWBS2, '' AS ConsolidateWBS3,
          max(LEVEL2.Name) AS BillWBS2Name, max(LEVEL3.Name) AS BillWBS3Name,
          '1/1/1900' AS FromDate, '1/1/1900' AS ToDate, 0 AS WBS2Rollup, 0 AS WBS3Rollup, '' As Description,
          sum(billExpenses.Extension) AS wbs3Amount,
          max(IsNull(BillPR.LongName, BillPR.Name)) As LongName,
          max(IsNull(LEVEL2.LongName, LEVEL2.Name)) As WBS2LongName,
          max(IsNull(LEVEL3.LongName, LEVEL3.Name)) As WBS3LongName,
          max(MainPR.BillingCurrencyCode) As CurrencyCode

          FROM
          PR AS BillPR,
          PR AS MainPR,
          PR AS LEVEL2,
          PR AS LEVEL3,
          (Select Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3, Sum(billExpDetail.Amount) AS Extension FROM billExpDetail
          WHERE @PreInvoice = 'N' And RateFlag = 'N' AND @showBillingBackup = 'Y' And billExpDetail.MainWBS1 = @parmMainWBS1
          AND billExpDetail.Invoice = @parmInvoice AND billExpDetail.Invoice = '&lt;Draft&gt;' And @parmShowBillingBackupExpense = 'Y'
          GROUP BY Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3
          Union All
          Select Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3, Sum(billConDetail.Amount) AS Extension FROM billConDetail
          WHERE @PreInvoice = 'N' And RateFlag = 'N' AND @showBillingBackup = 'Y' And billConDetail.MainWBS1 = @parmMainWBS1
          AND billConDetail.Invoice = @parmInvoice AND billConDetail.Invoice = '&lt;Draft&gt;' And @parmShowBillingBackupExpense = 'Y'
          GROUP BY Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3
          Union All
          Select Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3, Sum(billUnitDetail.Amount) AS Extension FROM billUnitDetail
          WHERE @PreInvoice = 'N' And RateFlag = 'N' AND @showBillingBackup = 'Y' And billUnitDetail.MainWBS1 = @parmMainWBS1
          AND billUnitDetail.Invoice = @parmInvoice AND billUnitDetail.Invoice = '&lt;Draft&gt;' And @parmShowBillingBackupExpense = 'Y'
          GROUP BY Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3) AS billExpenses

          WHERE @PreInvoice = 'N' And (LEVEL3.WBS1 = billExpenses.BillWBS1) AND (LEVEL3.WBS2 = billExpenses.BillWBS2) AND (LEVEL3.WBS3 = billExpenses.BillWBS3)
          AND (LEVEL2.WBS1 = billExpenses.BillWBS1) AND (LEVEL2.WBS2 = billExpenses.BillWBS2) AND (LEVEL2.WBS3 = /*N*/' ')
          AND (BillPR.WBS1 = billExpenses.BillWBS1) AND (BillPR.WBS2 = /*N*/' ')
          AND (MainPR.WBS1 = billExpenses.MainWBS1) AND (MainPR.WBS2 = /*N*/' ')
          AND billExpenses.MainWBS1 = @parmMainWBS1 AND @showBillingBackup = 'Y' AND billExpenses.Invoice = '&lt;Draft&gt;'
          Group By billExpenses.MainWBS1, billExpenses.BillWBS1, billExpenses.BillWBS2, billExpenses.BillWBS3

          ) as billInvUnion Group by MainWBS1, BillWBS1, BillWBS2, BillWBS3 ) as billBack

          /*Need to extend out the Labor Amounts and get totals for inv/wbs1/wbs2/wbs3*/
          /*All total queries must be unions to include records with sums (items that are billable)*/
          /*as well as the non billable labor items that would only be in billLabDetail*/

          /*SubQuery to get wbs2 totals*/
          LEFT JOIN (SELECT
          sumInvoice as sumInvoice,
          sumMainWBS1 as sumMainWBS1,
          sumBillWBS1 as sumBillWBS1,
          sumBillWBS2 as sumBillWBS2,
          sum(wbs2Amount) AS wbs2Amount FROM
          (SELECT billLabDetail.invoice as sumInvoice,
          billLabDetail.mainWBS1 as sumMainWBS1,
          billLabDetail.BillWBS1 as sumBillWBS1,
          billLabDetail.BillWBS2 as sumBillWBS2,
          sum(billLabDetail.FullExtension) AS wbs2Amount
          FROM billLabDetail
          WHERE @PreInvoice = 'N' And  FixedFee = 'Y' And billLabDetail.MainWBS1 = @parmMainWBS1
          AND billLabDetail.Invoice = @parmInvoice
          AND @parmShowLabExt = 'Y' AND @showBillingBackup = 'Y' And @parmShowBillingBackupLabor = 'Y'
          GROUP BY billLabDetail.invoice, billLabDetail.mainWBS1,
          billLabDetail.BillWBS1, billLabDetail.BillWBS2

          UNION ALL SELECT billExpDetail.invoice as sumInvoice,
          billExpDetail.mainWBS1 as sumMainWBS1,
          billExpDetail.BillWBS1 as sumBillWBS1,
          billExpDetail.BillWBS2 as sumBillWBS2,
          sum(billExpDetail.Amount) AS wbs2Amount
          FROM billExpDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billExpDetail.MainWBS1 = @parmMainWBS1
          AND billExpDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billExpDetail.invoice, billExpDetail.mainWBS1,
          billExpDetail.BillWBS1, billExpDetail.BillWBS2

          UNION ALL SELECT billConDetail.invoice as sumInvoice,
          billConDetail.mainWBS1 as sumMainWBS1,
          billConDetail.BillWBS1 as sumBillWBS1,
          billConDetail.BillWBS2 as sumBillWBS2,
          sum(billConDetail.Amount) AS wbs2Amount
          FROM billConDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billConDetail.MainWBS1 = @parmMainWBS1
          AND billConDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billConDetail.invoice, billConDetail.mainWBS1,
          billConDetail.BillWBS1, billConDetail.BillWBS2

          UNION ALL SELECT billUnitDetail.invoice as sumInvoice,
          billUnitDetail.mainWBS1 as sumMainWBS1,
          billUnitDetail.BillWBS1 as sumBillWBS1,
          billUnitDetail.BillWBS2 as sumBillWBS2,
          sum(billUnitDetail.Amount) AS wbs2Amount
          FROM billUnitDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billUnitDetail.MainWBS1 = @parmMainWBS1
          AND billUnitDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billUnitDetail.invoice, billUnitDetail.mainWBS1,
          billUnitDetail.BillWBS1, billUnitDetail.BillWBS2

          UNION ALL SELECT
          billInvSums.invoice as sumInvoice,
          billInvSums.mainWBS1 as sumMainWBS1,
          billInvSums.BillWBS1 as sumBillWBS1,
          billInvSums.BillWBS2 as sumBillWBS2,
          sum(billInvSums.finalAmount) AS wbs2Amount
          FROM billInvSums
          WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I'
          And (Section In (/*N*/'L', /*N*/'E', /*N*/'C', /*N*/'U') Or (Section = /*N*/'F' And @parmShowBackupFees = 'Y') Or (Section In (/*N*/'A',/*N*/'Retainer') And @parmShowBackupAddOns = 'Y'))
            And billInvSums.MainWBS1 = @parmMainWBS1
          AND billInvSums.Invoice = @parmInvoice
          AND @showBillingBackup = 'Y'
          GROUP BY billInvSums.invoice, billInvSums.mainWBS1,
          billInvSums.BillWBS1, billInvSums.BillWBS2) AS wbs2Union
          GROUP BY sumInvoice, sumMainWBS1, sumBillWBS1, sumBillWBS2) AS wbs2Section
          ON billBack.MainWBS1 = wbs2Section.sumMainWBS1 AND
          billBack.Invoice = wbs2Section.sumInvoice AND billBack.BillWBS1 = wbs2Section.sumBillWBS1
          AND billBack.BillWBS2 = wbs2Section.sumBillWBS2

          /*SubQuery to get wbs1 totals*/
          LEFT JOIN (SELECT
          sumInvoice as sumInvoice,
          sumMainWBS1 as sumMainWBS1,
          sumBillWBS1 as sumBillWBS1,
          sum(wbs1Amount) AS wbs1Amount FROM
          (SELECT
          billLabDetail.invoice as sumInvoice,
          billLabDetail.mainWBS1 as sumMainWBS1,
          billLabDetail.BillWBS1 as sumBillWBS1,
          sum(billLabDetail.FullExtension) AS wbs1Amount
          FROM billLabDetail
          WHERE @PreInvoice = 'N' And FixedFee = 'Y' AND billLabDetail.MainWBS1 = @parmMainWBS1
          AND billLabDetail.Invoice = @parmInvoice
          AND @parmShowLabExt = 'Y' AND @showBillingBackup = 'Y' And @parmShowBillingBackupLabor= 'Y'
          GROUP BY billLabDetail.invoice, billLabDetail.mainWBS1,
          billLabDetail.BillWBS1

          UNION ALL SELECT billExpDetail.invoice as sumInvoice,
          billExpDetail.mainWBS1 as sumMainWBS1,
          billExpDetail.BillWBS1 as sumBillWBS1,
          sum(billExpDetail.Amount) AS wbs1Amount
          FROM billExpDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billExpDetail.MainWBS1 = @parmMainWBS1
          AND billExpDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billExpDetail.invoice, billExpDetail.mainWBS1,
          billExpDetail.BillWBS1

          UNION ALL SELECT billConDetail.invoice as sumInvoice,
          billConDetail.mainWBS1 as sumMainWBS1,
          billConDetail.BillWBS1 as sumBillWBS1,
          sum(billConDetail.Amount) AS wbs1Amount
          FROM billConDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billConDetail.MainWBS1 = @parmMainWBS1
          AND billConDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billConDetail.invoice, billConDetail.mainWBS1,
          billConDetail.BillWBS1

          UNION ALL SELECT billUnitDetail.invoice as sumInvoice,
          billUnitDetail.mainWBS1 as sumMainWBS1,
          billUnitDetail.BillWBS1 as sumBillWBS1,
          sum(billUnitDetail.Amount) AS wbs1Amount
          FROM billUnitDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billUnitDetail.MainWBS1 = @parmMainWBS1
          AND billUnitDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billUnitDetail.invoice, billUnitDetail.mainWBS1,
          billUnitDetail.BillWBS1

          UNION ALL SELECT
          billInvSums.invoice as sumInvoice,
          billInvSums.mainWBS1 as sumMainWBS1,
          billInvSums.BillWBS1 as sumBillWBS1,
          sum(billInvSums.finalAmount) AS wbs1Amount
          FROM billInvSums
          WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I'
          And (Section In (/*N*/'L', /*N*/'E', /*N*/'C', /*N*/'U') Or (Section = /*N*/'F' And @parmShowBackupFees = 'Y') Or (Section In (/*N*/'A',/*N*/'Retainer') And @parmShowBackupAddOns = 'Y'))
          And billInvSums.MainWBS1 = @parmMainWBS1
          AND billInvSums.Invoice = @parmInvoice
          AND @showBillingBackup = 'Y'
          GROUP BY billInvSums.invoice, billInvSums.mainWBS1,
          billInvSums.BillWBS1) AS wbs1Union
          GROUP BY sumInvoice, sumMainWBS1, sumBillWBS1) AS wbs1Section ON billBack.MainWBS1 = wbs1Section.sumMainWBS1 AND
          billBack.Invoice = wbs1Section.sumInvoice AND billBack.BillWBS1 = wbs1Section.sumBillWBS1

          /*SubQuery to get invoice totals*/
          LEFT JOIN (SELECT
          sumInvoice as sumInvoice,
          sumMainWBS1 as sumMainWBS1,
          sum(invAmount) AS invAmount FROM
          (SELECT
          billLabDetail.invoice as sumInvoice,
          billLabDetail.mainWBS1 as sumMainWBS1,
          sum(billLabDetail.FullExtension) AS invAmount
          FROM billLabDetail
          WHERE @PreInvoice = 'N' And FixedFee = 'Y' AND billLabDetail.MainWBS1 = @parmMainWBS1
          AND billLabDetail.Invoice = @parmInvoice
          AND @parmShowLabExt = 'Y' AND @showBillingBackup = 'Y' And @parmShowBillingBackupLabor= 'Y'
          GROUP BY billLabDetail.Invoice, billLabDetail.mainWBS1

          UNION ALL SELECT billExpDetail.invoice as sumInvoice,
          billExpDetail.mainWBS1 as sumMainWBS1,
          sum(billExpDetail.Amount) AS invAmount
          FROM billExpDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billExpDetail.MainWBS1 = @parmMainWBS1
          AND billExpDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billExpDetail.invoice, billExpDetail.mainWBS1

          UNION ALL SELECT billConDetail.invoice as sumInvoice,
          billConDetail.mainWBS1 as sumMainWBS1,
          sum(billConDetail.Amount) AS invAmount
          FROM billConDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billConDetail.MainWBS1 = @parmMainWBS1
          AND billConDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billConDetail.invoice, billConDetail.mainWBS1

          UNION ALL SELECT billUnitDetail.invoice as sumInvoice,
          billUnitDetail.mainWBS1 as sumMainWBS1,
          sum(billUnitDetail.Amount) AS invAmount
          FROM billUnitDetail
          WHERE @PreInvoice = 'N' And Invoice = '&lt;Draft&gt;' And RateFlag = 'N' And billUnitDetail.MainWBS1 = @parmMainWBS1
          AND billUnitDetail.Invoice = @parmInvoice AND @showBillingBackup = 'Y' And @parmShowBillingBackupExpense= 'Y'
          GROUP BY billUnitDetail.invoice, billUnitDetail.mainWBS1

          UNION ALL SELECT
          billInvSums.invoice as sumInvoice,
          billInvSums.mainWBS1 as sumMainWBS1,
          sum(billInvSums.finalAmount) AS invAmount
          FROM billInvSums
          WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' AND @showBillingBackup = 'Y'
          And (Section In (/*N*/'L', /*N*/'E', /*N*/'C', /*N*/'U') Or (Section = /*N*/'F' And @parmShowBackupFees = 'Y') Or (Section In (/*N*/'A',/*N*/'Retainer') And @parmShowBackupAddOns = 'Y'))
          And billInvSums.MainWBS1 = @parmMainWBS1
          AND billInvSums.Invoice = @parmInvoice
          GROUP BY billInvSums.invoice, billInvSums.mainWBS1) AS invUnion
          GROUP BY sumInvoice, sumMainWBS1) AS invSection ON billBack.MainWBS1 = invSection.sumMainWBS1 AND
          billBack.Invoice = invSection.sumInvoice
          WHERE @PreInvoice = 'N' AND @showBillingBackup = 'Y'
          Group by Invoice, MainWBS1, BillWBS1, BillWBS2, BillWBS3
          /*the having clause filters out phases/tasks when no labor/detail items exist in the bill tables (so a phase total does not exist without detail)*/
          Having (max(NonBill) &gt; 0 Or sum(invSection.invAmount) is not Null) AND (max(NonBill) &gt; 0 Or sum(wbs1Section.wbs1Amount) is not Null) AND (max(NonBill) &gt; 0 Or sum(wbs2Section.wbs2Amount) is not Null)
          AND (max(NonBill) &gt; 0 Or sum(wbs3Total) is not Null)

          ORDER BY MainType, BillWBS1, BillWBS2, BillWBS3</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
