      <Query>
        <DataSourceName>VisionQA51MCMC__alisond_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@parmInvoice">
            <Value>=Parameters!parmInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BillingCurrencyCode">
            <Value>=Parameters!BillingCurrencyCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PreInvoice">
            <Value>=Parameters!PreInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmMainWBS1">
            <Value>=Parameters!parmMainWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS1">
            <Value>=Parameters!parmWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS2">
            <Value>=Parameters!parmWBS2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS3">
            <Value>=Parameters!parmWBS3.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT sortKey,
CASE WHEN method = 3 AND transType = /*N*/'EX' THEN description1 ELSE sortLabel END AS sortLabel,
transDate,
description1,
description2,
invoiceNumber,
invoiceDate,
transType,
refno,
amount,
multiplier,
showMult,
expenseDetail,
diffMult,
SortKey2,
CASE WHEN method = 3 AND transType = /*N*/'EX' THEN description1 ELSE sortLabel2 END AS sortLabel2,
IsNull(summary2.subAmount, 0) As SubSectionTotal,
IsNull(summary.finalAmount, 0) As SectionTotal,
'' As Reason,
'' As Miles,
'' As AmountPerMile,
'' As DistanceType,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
,billExpDetail.ponumber
,billExpDetail.quantity
,billExpDetail.rate
,billExpDetail.UOM
,billExpDetail.ShowPODetail
FROM billExpDetail
left join
(SELECT
billInvSums.invoice as sumInvoice,
billInvSums.mainWBS1 as sumMainWBS1,
billInvSums.BillWBS1 as sumBillWBS1,
billInvSums.BillWBS2 as sumBillWBS2,
billInvSums.BillWBS3 as sumBillWBS3,
sum(billInvSums.finalAmount) AS finalAmount
FROM billInvSums
WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' AND Section = /*N*/'E'
AND (Invoice = @parmInvoice)
AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~')
AND (BillWBS1 = @parmWBS1 OR BillWBS1 = /*N*/'~')
AND (BillWBS2 = @parmWBS2 OR BillWBS2 = /*N*/'~')
AND (BillWBS3 = @parmWBS3 OR BillWBS3 = /*N*/'~')
GROUP BY billInvSums.invoice,
billInvSums.mainWBS1,
billInvSums.BillWBS1,
billInvSums.BillWBS2,
billInvSums.BillWBS3
) AS summary ON billExpDetail.invoice = summary.sumInvoice
AND billExpDetail.mainWBS1 = summary.sumMainWBS1
AND billExpDetail.BillWBS1 = summary.sumBillWBS1
AND billExpDetail.BillWBS2 = summary.sumBillWBS2
AND billExpDetail.BillWBS3 = summary.sumBillWBS3
left join
(SELECT
billInvSums.invoice as sumInvoice,
billInvSums.mainWBS1 as sumMainWBS1,
billInvSums.BillWBS1 as sumBillWBS1,
billInvSums.BillWBS2 as sumBillWBS2,
billInvSums.BillWBS3 as sumBillWBS3,
billInvSums.subSection as sumSubSection,
sum(billInvSums.finalAmount) AS subAmount
FROM billInvSums
WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' AND Section = /*N*/'E'
AND (Invoice = @parmInvoice)
AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~')
AND (BillWBS1 = @parmWBS1 OR BillWBS1 = /*N*/'~')
AND (BillWBS2 = @parmWBS2 OR BillWBS2 = /*N*/'~')
AND (BillWBS3 = @parmWBS3 OR BillWBS3 = /*N*/'~')
GROUP BY billInvSums.invoice,
billInvSums.mainWBS1,
billInvSums.BillWBS1,
billInvSums.BillWBS2,
billInvSums.BillWBS3,
billInvSums.subSection
) AS summary2 ON (billExpDetail.sortKey = summary2.sumSubSection OR summary2.sumSubSection = '' OR summary2.sumSubSection = '0')
AND billExpDetail.invoice = summary2.sumInvoice
AND billExpDetail.mainWBS1 = summary2.sumMainWBS1
AND billExpDetail.BillWBS1 = summary2.sumBillWBS1
AND billExpDetail.BillWBS2 = summary2.sumBillWBS2
AND billExpDetail.BillWBS3 = summary2.sumBillWBS3
WHERE @PreInvoice = 'N' And IsNull(RateFlag, '') &lt;&gt; 'N' And billExpDetail.Amount &lt;&gt; 0 
AND (Invoice = @parmInvoice) 
AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
AND (billExpDetail.BillWBS1 = @parmWBS1 OR billExpDetail.BillWBS1 = /*N*/'~') 
AND (billExpDetail.BillWBS2 = @parmWBS2 OR billExpDetail.BillWBS2 = /*N*/'~') 
AND (billExpDetail.BillWBS3 = @parmWBS3 OR billExpDetail.BillWBS3 = /*N*/'~') 
ORDER BY sortKey, transDate</CommandText>
