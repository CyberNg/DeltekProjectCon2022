 (billTaxDetail.mainWBS1 = summary.sumMainWBS1) AND 
 (billTaxDetail.BillWBS1 = summary.sumBillWBS1) 

/*SubQuery to get PreInvoice totals*/
left join  (SELECT 
  billPreInvoice.invoice as sumInvoice,
  billPreInvoice.WBS1 as sumMainWBS1,
  sum(billPreInvoice.AppliedAmount) AS AppliedAmount
  FROM billPreInvoice
  WHERE billPreInvoice.WBS1 = IsNull(@parmMainWBS1, /*N*/'')
AND billPreInvoice.Invoice = IsNull(@parmInvoice, /*N*/'')
/***EXTEND WHERE CLAUSE***/
 GROUP BY billPreInvoice.invoice, billPreInvoice.WBS1) AS preInvTot ON 
 (billTaxDetail.invoice = preInvTot.sumInvoice) AND 
 (billTaxDetail.mainWBS1 = preInvTot.sumMainWBS1)

