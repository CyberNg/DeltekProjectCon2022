        <CommandText>/******************************************************************** WBS3 ***********************************************/

SELECT 
billTaxDetail.RecdSeq,
billTaxDetail.code,
billTaxDetail.description,
computation,
rate,
basis,
amount, 
TaxCurrencyCode, AmountTaxCurrency,
IsNull(summary.TaxSub, 0) As TaxSubtotal,
IsNull(summary.SubTaxCurrency, 0) As SubtotalTaxCurrency,
IsNull(summary.sumTaxCurrencyCode, '') AS subTaxCurrencyCode,
IsNull(summary.finalAmount, 0) As SectionTotal,
0 As AppliedTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode,
CAST(@DecimalPlaces as int) As DecimalPlaces
 FROM billTaxDetail left join
/*SubQuery to get invoice totals*/
 (SELECT 
  billInvSums.invoice as sumInvoice,
  billInvSums.mainWBS1 as sumMainWBS1,
  billInvSums.BillWBS1 as sumBillWBS1,
billInvSums.BillWBS2 as sumBillWBS2,  
billInvSums.BillWBS3 as sumBillWBS3,
  sum(CASE WHEN (Section &lt;&gt; 'T') THEN billInvSums.finalAmount ELSE 0 END) AS TaxSub,
  sum(CASE WHEN (Section &lt;&gt; 'T') THEN billInvSums.FinalAmountTaxCurrency ELSE 0 END) AS SubTaxCurrency,
  min(billInvSums.TaxCurrencyCode) AS sumTaxCurrencyCode,
  sum(CASE WHEN (Section = 'T') THEN billInvSums.finalAmount ELSE 0 END) AS finalAmount
  FROM billInvSums 
  WHERE
ArrayType = 'I' AND PreInvoice = 'N' /***taxSubTotal***/
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 AND (BillWBS1 = @parmWBS1 OR BillWBS1 = /*N*/'~') 
 AND (BillWBS2 = @parmWBS2 OR BillWBS2 = /*N*/'~') 
 AND (BillWBS3 = @parmWBS3 OR BillWBS3 = /*N*/'~') 
 GROUP BY billInvSums.invoice, billInvSums.mainWBS1, 
 billInvSums.BillWBS1, billInvSums.BillWBS2, billInvSums.BillWBS3) AS summary ON 
 (billTaxDetail.invoice = summary.sumInvoice) AND 
 (billTaxDetail.mainWBS1 = summary.sumMainWBS1) AND 
 (billTaxDetail.BillWBS1 = summary.sumBillWBS1) AND
 (billTaxDetail.BillWBS2 = summary.sumBillWBS2) AND 
 (billTaxDetail.BillWBS3 = summary.sumBillWBS3)

 WHERE RetainageInvoice &lt;&gt; 'Y' AND PreInvoice = 'N'
 AND (billTaxDetail.Invoice = @parmInvoice) 
 AND (billTaxDetail.MainWBS1 = @parmMainWBS1 OR billTaxDetail.MainWBS1 = /*N*/'~') 
 AND (billTaxDetail.BillWBS1 = @parmWBS1 OR billTaxDetail.BillWBS1 = /*N*/'~') 
 AND (billTaxDetail.BillWBS2 = @parmWBS2 OR billTaxDetail.BillWBS2 = /*N*/'~') 
 AND (billTaxDetail.BillWBS3 = @parmWBS3 OR billTaxDetail.BillWBS3 = /*N*/'~') 
And billTaxDetail.billWBS3 &lt;&gt; /*N*/'~' And billTaxDetail.billWBS3 &lt;&gt; /*N*/' ' 
And @parmOnWBS3 = 'Y'

/******************************************************************** WBS2 ***********************************************/

