SELECT Ar.preInvoice as invoiceNumber,AR.invoiceDate as invoiceDate,max(AR.Note) as note,
sum(ARD.amount) as amount, sum(ARD.paidAmount) as paid, sum(case when AR.Cancelled = 'Y' And AppliedInvoice is null then ARD.amount else (case when AppliedInvoice is not null And ARD.amount-ARD.paidAmount &gt; 0 then ARD.amount-ARD.paidAmount else 0 end) end) as cancelled, sum(IsNull(billPreInvoice.AppliedAmount, 0)) as applied,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  ARPreInvoice AR inner join ARPreInvoiceDetail ARD on AR.WBS1 = ARD.WBS1 AND 
AR.WBS2 = ARD.WBS2 AND AR.WBS3 = ARD.WBS3 AND AR.PreInvoice = ARD.PreInvoice 
left join billPreInvoice on AR.WBS1 = billPreInvoice.WBS1 AND 
AR.PreInvoice = billPreInvoice.PreInvoice AND billPreInvoice.Invoice = @parmInvoice

left join (
Select billInvMaster.mainWBS1, billInvMaster.invoice as invInvoice,billGroup
From billInvMaster  
WHERE PreInvoice = 'Y' And (billInvMaster.MainWBS1 = @parmMainWBS1) 
and MainType &lt;&gt; 'S'
Group By billInvMaster.mainWBS1, billInvMaster.invoice, billGroup) As billInv On (AR.WBS1 = billInv.MainWBS1) And (AR.PreInvoice = billInv.invInvoice) 

WHERE @PreInvoice = 'Y' And billInv.billGroup = 'N' And @parmOnWBS1 = 'Y' And AR.PreInvoice  &lt;&gt; '&lt;Draft&gt;' And
((@parmInvoice &lt;&gt; '&lt;Draft&gt;' And (AR.PreInvoice &lt; CASE WHEN IsNumeric(@parmInvoice)=1
        THEN CAST(@parmInvoice AS INT)
        ELSE AR.PreInvoice+1
    END
))
 Or (@parmInvoice = '&lt;Draft&gt;' And AppliedInvoice is Null))


And @PreInvoiceEnabled = 'Y' And ARD.InvoiceSection = 'F'
AND ARD.WBS1 = @parmMainWBS1 AND amount &lt;&gt; 0 
GROUP BY AR.preInvoice, AR.invoiceDate

/*** Final Invoice Query***/
UNION ALL SELECT Ar.preInvoice as invoiceNumber,AR.invoiceDate as invoiceDate,max(AR.Note) as note,
sum(ARD.amount) as amount, sum(ARD.paidAmount) as paid, sum(case when ARD.amount-ARD.paidAmount &gt; 0 then ARD.amount-ARD.paidAmount else 0 end) as cancelled, sum(IsNull(billPreInvoice.AppliedAmount, 0)) as applied,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  ARPreInvoice AR inner join ARPreInvoiceDetail ARD on AR.WBS1 = ARD.WBS1 AND 
AR.WBS2 = ARD.WBS2 AND AR.WBS3 = ARD.WBS3 AND AR.PreInvoice = ARD.PreInvoice 
left join billPreInvoice on AR.WBS1 = billPreInvoice.WBS1 AND 
AR.PreInvoice = billPreInvoice.PreInvoice AND billPreInvoice.Invoice = @parmInvoice

left join (
Select billInvMaster.mainWBS1, billInvMaster.invoice as invInvoice,billGroup
From billInvMaster  
WHERE PreInvoice = 'N' AND (billInvMaster.MainWBS1 = @parmMainWBS1) 
and MainType &lt;&gt; 'S'
Group By billInvMaster.mainWBS1, billInvMaster.invoice, billGroup) As billInv On (billPreInvoice.WBS1 = billInv.MainWBS1) And (billPreInvoice.Invoice = billInv.invInvoice) 

WHERE @PreInvoice &lt;&gt; 'Y' And billInv.billGroup = 'N' And @parmOnWBS1 = 'Y' And
(AppliedInvoice = @parmInvoice
 Or (@parmInvoice = '&lt;Draft&gt;' And AppliedInvoice is Null))

And @PreInvoiceEnabled = 'Y' And ARD.InvoiceSection = 'F'
AND ARD.WBS1 = @parmMainWBS1 AND amount &lt;&gt; 0 
GROUP BY AR.preInvoice, AR.invoiceDate

/*******************************GROUP ***************************************/
/***PreInvoice Query ***/

UNION SELECT Ar.preInvoice as invoiceNumber,AR.invoiceDate as invoiceDate,max(AR.Note) as note,
sum(ARD.amount) as amount, sum(ARD.paidAmount) as paid, sum(case when AR.Cancelled = 'Y' then ARD.amount else (case when AppliedInvoice is not null And ARD.amount-ARD.paidAmount &gt; 0 then ARD.amount-ARD.paidAmount else 0 end) end) as cancelled, sum(IsNull(billPreInvoice.AppliedAmount, 0)) as applied,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  ARPreInvoice AR inner join ARPreInvoiceDetail ARD on AR.WBS1 = ARD.WBS1 AND 
AR.WBS2 = ARD.WBS2 AND AR.WBS3 = ARD.WBS3 AND AR.PreInvoice = ARD.PreInvoice 
left join billPreInvoice on AR.WBS1 = billPreInvoice.WBS1 AND 
AR.PreInvoice = billPreInvoice.PreInvoice AND billPreInvoice.Invoice = @parmInvoice

left join (
Select billInvMaster.mainWBS1, billInvMaster.invoice as invInvoice,billGroup
From billInvMaster  
WHERE PreInvoice = 'Y' And (billInvMaster.MainWBS1 = @parmMainWBS1 OR billInvMaster.MainWBS1 = /*N*/'~') 
and MainType &lt;&gt; 'S'
Group By billInvMaster.mainWBS1, billInvMaster.invoice, billGroup) As billInv On (AR.WBS1 = billInv.MainWBS1) And (AR.PreInvoice = billInv.invInvoice) 

WHERE @PreInvoice = 'Y' And billInv.billGroup = 'Y' And @parmOnGroup = 'Y' And AR.PreInvoice  &lt;&gt; '&lt;Draft&gt;' And
((@parmInvoice &lt;&gt; '&lt;Draft&gt;' And  (AR.PreInvoice &lt; CASE WHEN IsNumeric(@parmInvoice)=1
        THEN CAST(@parmInvoice AS INT)
        ELSE AR.PreInvoice+1
    END
))
 Or (@parmInvoice = '&lt;Draft&gt;' And AppliedInvoice is Null))


And @PreInvoiceEnabled = 'Y' And ARD.InvoiceSection = 'F'
AND ARD.WBS1 = @parmMainWBS1 AND amount &lt;&gt; 0 
GROUP BY AR.preInvoice, AR.invoiceDate

/*** Final Invoice Query***/
UNION ALL SELECT Ar.preInvoice as invoiceNumber,AR.invoiceDate as invoiceDate,max(AR.Note) as note,
sum(ARD.amount) as amount, sum(ARD.paidAmount) as paid, sum(case when ARD.amount-ARD.paidAmount &gt; 0 then ARD.amount-ARD.paidAmount else 0 end) as cancelled, sum(IsNull(billPreInvoice.AppliedAmount, 0)) as applied,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  ARPreInvoice AR inner join ARPreInvoiceDetail ARD on AR.WBS1 = ARD.WBS1 AND 
AR.WBS2 = ARD.WBS2 AND AR.WBS3 = ARD.WBS3 AND AR.PreInvoice = ARD.PreInvoice 
left join billPreInvoice on AR.WBS1 = billPreInvoice.WBS1 AND 
AR.PreInvoice = billPreInvoice.PreInvoice AND billPreInvoice.Invoice = @parmInvoice

left join (
Select billInvMaster.mainWBS1, billInvMaster.invoice as invInvoice,billGroup
From billInvMaster  
WHERE PreInvoice = 'N' AND (billInvMaster.MainWBS1 = @parmMainWBS1 OR billInvMaster.MainWBS1 = /*N*/'~') 
and MainType &lt;&gt; 'S'
Group By billInvMaster.mainWBS1, billInvMaster.invoice, billGroup) As billInv On (billPreInvoice.WBS1 = billInv.MainWBS1) And (billPreInvoice.Invoice = billInv.invInvoice) 

WHERE @PreInvoice &lt;&gt; 'Y' And billInv.billGroup = 'Y' And @parmOnGroup = 'Y' And
(AppliedInvoice = @parmInvoice
 Or (@parmInvoice = '&lt;Draft&gt;' And AppliedInvoice is Null))

And @PreInvoiceEnabled = 'Y' And ARD.InvoiceSection = 'F'
AND ARD.WBS1 = @parmMainWBS1 AND amount &lt;&gt; 0 
GROUP BY AR.preInvoice, AR.invoiceDate

 ORDER BY Ar.preInvoice</CommandText>
