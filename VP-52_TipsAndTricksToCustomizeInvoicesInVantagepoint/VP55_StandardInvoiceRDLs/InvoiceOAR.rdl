        <CommandText>/**********************************************************WBS3****************************************************************/

SELECT 
invoiceNumber,
invoiceDate,
sum(balance) as balance,
sum(retainage) as retainage,
0 as received,
retainageFormat,
'' As printWBS2AR,
'' As printARWBS2Totals,
'' As billGroup,
'' As printWBS1AR,
IsNull(summary.finalAmount, 0) As SectionTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
 FROM billARDetail left join

/****SubQuery to get invoice totals****/
 (SELECT 
  billInvSums.invoice as sumInvoice,
  billInvSums.mainWBS1 as sumMainWBS1,
  billInvSums.BillWBS1 as sumBillWBS1,
  billInvSums.BillWBS2 as sumBillWBS2,
  billInvSums.BillWBS3 as sumBillWBS3,
  sum(billInvSums.finalAmount) AS finalAmount
  FROM billInvSums 
  WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And 
ArrayType = 'I' AND 
(billInvSums.Invoice = @parmInvoice) 
AND (billInvSums.MainWBS1 = @parmMainWBS1 OR billInvSums.MainWBS1 = /*N*/'~') 
AND (billInvSums.BillWBS1 = @parmWBS1 OR billInvSums.BillWBS1 = /*N*/'~') 
AND (billInvSums.BillWBS2 = @parmWBS2 OR billInvSums.BillWBS2 = /*N*/'~') 
AND (billInvSums.BillWBS3 = @parmWBS3 OR billInvSums.BillWBS3 = /*N*/'~')
 GROUP BY billInvSums.invoice, billInvSums.mainWBS1,
 billInvSums.BillWBS1, billInvSums.BillWBS2, billInvSums.BillWBS3) AS summary ON 
 (billARDetail.invoice = summary.sumInvoice) AND 
 (billARDetail.mainWBS1 = summary.sumMainWBS1) AND 
 (billARDetail.BillWBS1 = summary.sumBillWBS1) AND 
 (billARDetail.BillWBS2 = summary.sumBillWBS2) AND 
 (billARDetail.BillWBS3 = summary.sumBillWBS3)
 WHERE @PreInvoice = 'N' And
(billARDetail.Invoice = @parmInvoice) 
AND (billARDetail.MainWBS1 = @parmMainWBS1 OR billARDetail.MainWBS1 = /*N*/'~') 
AND (billARDetail.BillWBS1 = @parmWBS1 OR billARDetail.BillWBS1 = /*N*/'~') 
AND (billARDetail.BillWBS2 = @parmWBS2 OR billARDetail.BillWBS2 = /*N*/'~') 
AND (billARDetail.BillWBS3 = @parmWBS3) AND (billARDetail.BillWBS3 &lt;&gt; /*N*/'~')
AND @parmOnWBS3 = 'Y'
 GROUP BY invoiceNumber, invoiceDate, retainageFormat,summary.finalAmount 


/********************************************************** WBS2 **********************************************************/

UNION ALL SELECT invoiceNumber,invoiceDate,
sum(balance) as balance,
sum(retainage) as retainage,
0 as received,
retainageFormat,
printAR.printWBS2AR,
printAR.printARWBS2Totals,
'' As billGroup,
'' As printWBS1AR,
IsNull(summary.finalAmount, 0) As SectionTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  billARDetail left join  (
Select billInvMaster.invoice as invInvoice,
billInvMaster.mainWBS1 as invMainWBS1,billInvMaster.BillWBS1 As invBillWBS1,billInvMaster.BillWBS2 AS invBillWBS2,
printWBS2AR, 
printARWBS2Totals
From billInvMaster  
Where @PreInvoice = 'N' And (billInvMaster.Invoice = @parmInvoice) 
AND (billInvMaster.MainWBS1 = @parmMainWBS1 OR billInvMaster.MainWBS1 = /*N*/'~') 
AND (billInvMaster.BillWBS1 = @parmWBS1 OR billInvMaster.BillWBS1 = /*N*/'~') 
AND (billInvMaster.BillWBS2 = @parmWBS2 OR billInvMaster.BillWBS2 = /*N*/'~') 
Group By billInvMaster.invoice, billInvMaster.mainWBS1, billInvMaster.BillWBS1, billInvMaster.BillWBS2, printWBS2AR, printARWBS2Totals
) As printAR On  (billARDetail.invoice = printAR.invInvoice) AND  (billARDetail.mainWBS1 = printAR.invMainWBS1) AND  (billARDetail.BillWBS1 = printAR.invBillWBS1) AND  (billARDetail.BillWBS2 = printAR.invBillWBS2)  
left  join  (SELECT   billInvSums.invoice as sumInvoice,  billInvSums.mainWBS1 as sumMainWBS1,  billInvSums.BillWBS1 as sumBillWBS1,  billInvSums.BillWBS2 as sumBillWBS2,  sum(billInvSums.finalAmount) AS finalAmount  
FROM billInvSums   
WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' 
AND (billInvSums.Invoice = @parmInvoice) 
AND (billInvSums.MainWBS1 = @parmMainWBS1 OR billInvSums.MainWBS1 = /*N*/'~') 
AND (billInvSums.BillWBS1 = @parmWBS1 OR billInvSums.BillWBS1 = /*N*/'~') 
AND (billInvSums.BillWBS2 = @parmWBS2 OR billInvSums.BillWBS2 = /*N*/'~') 
GROUP BY billInvSums.invoice, billInvSums.mainWBS1,  billInvSums.BillWBS1, billInvSums.BillWBS2
) AS summary ON  (billARDetail.invoice = summary.sumInvoice) AND  (billARDetail.mainWBS1 = summary.sumMainWBS1) AND  (billARDetail.BillWBS1= summary.sumBillWBS1) AND  (billARDetail.BillWBS2 = summary.sumBillWBS2)  
WHERE @PreInvoice = 'N' And (billARDetail.Invoice = @parmInvoice) 
AND (billARDetail.MainWBS1 = @parmMainWBS1 OR billARDetail.MainWBS1 = /*N*/'~') 
AND (billARDetail.BillWBS1 = @parmWBS1 OR billARDetail.BillWBS1 = /*N*/'~') 
AND (billARDetail.BillWBS2 = @parmWBS2 OR billARDetail.BillWBS2 = /*N*/'~') 
AND billARDetail.billwbs2 &lt;&gt; /*N*/'~' And billARDetail.billWBS2 &lt;&gt; /*N*/' ' And (billARDetail.billWBS3 = /*N*/' ' Or billARDetail.billWBS3 = /*N*/'~' Or printARWBS2Totals = 'Y')  
AND @parmOnWBS2 = 'Y'
GROUP BY invoiceNumber, invoiceDate, retainageFormat, printAR.printWBS2AR, printAR.printARWBS2Totals,summary.finalAmount  

/********************************************************** WBS1**************************************************/
UNION ALL SELECT invoiceNumber,invoiceDate,
sum(balance) as balance,
sum(retainage) as retainage,
0 as received,
retainageFormat,
'' As printWBS2AR,
'' As printARWBS2Totals,
'' As billGroup,
printAR.printWBS1AR,
IsNull(summary.finalAmount, 0) As SectionTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  billARDetail left join
(Select billInvMaster.invoice as invInvoice,
billInvMaster.mainWBS1 as invMainWBS1,billInvMaster.BillWBS1 As invBillWBS1,
printWBS1AR
From billInvMaster  
Where @PreInvoice = 'N' And (billInvMaster.Invoice = @parmInvoice) 
AND (billInvMaster.MainWBS1 = @parmMainWBS1 OR billInvMaster.MainWBS1 = /*N*/'~') 
AND (billInvMaster.BillWBS1 = @parmWBS1 OR billInvMaster.BillWBS1 = /*N*/'~') 
Group By billInvMaster.invoice, billInvMaster.mainWBS1, billInvMaster.BillWBS1, printWBS1AR
) As printAR On  (billARDetail.invoice = printAR.invInvoice) AND  (billARDetail.mainWBS1 = printAR.invMainWBS1) AND  (billARDetail.BillWBS1 = printAR.invBillWBS1) 

/****SubQuery to get invoice totals****/
left  join  (SELECT   billInvSums.invoice as sumInvoice,  billInvSums.mainWBS1 as sumMainWBS1,  billInvSums.BillWBS1 as sumBillWBS1,  
sum(billInvSums.finalAmount) AS finalAmount  
FROM billInvSums   
WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' 
AND (billInvSums.Invoice = @parmInvoice) 
AND (billInvSums.MainWBS1 = @parmMainWBS1 OR billInvSums.MainWBS1 = /*N*/'~') 
AND (billInvSums.BillWBS1 = @parmWBS1 OR billInvSums.BillWBS1 = /*N*/'~') 
GROUP BY billInvSums.invoice, billInvSums.mainWBS1,  billInvSums.BillWBS1
) AS summary ON  (billARDetail.invoice = summary.sumInvoice) AND  (billARDetail.mainWBS1 = summary.sumMainWBS1) AND  (billARDetail.BillWBS1= summary.sumBillWBS1)
WHERE @PreInvoice = 'N' And (billARDetail.Invoice = @parmInvoice) 
AND (billARDetail.MainWBS1 = @parmMainWBS1 OR billARDetail.MainWBS1 = /*N*/'~') 
AND (billARDetail.BillWBS1 = @parmWBS1 OR billARDetail.BillWBS1 = /*N*/'~')  
AND billARDetail.billwbs1 &lt;&gt; /*N*/'~' And (billARDetail.billWBS2 = /*N*/' ' Or billARDetail.billWBS2 = /*N*/'~' Or printWBS1AR = 'Y')
AND @parmOnWBS1 = 'Y'
GROUP BY invoiceNumber, invoiceDate, retainageFormat, printAR.printWBS1AR,summary.finalAmount

/**********************************************************GROUP ******************************************************/

UNION ALL SELECT invoiceNumber,invoiceDate,
sum(balance) as balance,
sum(retainage) as retainage,
min(PreInvRecieved) as received,
retainageFormat,
'' As printWBS2AR,
'' As printARWBS2Totals,
printAR.billGroup,
printAR.printWBS1AR,
IsNull(summary.finalAmount, 0) As SectionTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM  billARDetail left join  
(Select billInvMaster.invoice as invInvoice,billGroup,printWBS1AR  
From billInvMaster  
Where @PreInvoice = 'N' And (billInvMaster.Invoice = @parmInvoice) 
AND (billInvMaster.MainWBS1 = @parmMainWBS1 OR billInvMaster.MainWBS1 = /*N*/'~') 
And MainType &lt;&gt; 'S' 
Group By billInvMaster.invoice, billInvMaster.billGroup, billInvMaster.printWBS1AR
)  As printAR On  (billARDetail.invoice = printAR.invInvoice)

 left join (
Select AR.WBS1, Sum(PaidAmount) As PreInvRecieved From ARPreInvoiceDetail ARD Inner Join ARPreInvoice AR On ARD.WBS1 = AR.WBS1 And ARD.WBS2 = AR.WBS2 And ARD.WBS3 = AR.WBS3 And ARD.PreInvoice = AR.PreInvoice Where ARD.InvoiceSection = 'F' And AR.WBS1 = @parmMainWBS1 And (AppliedInvoice = @parmInvoice Or (AppliedInvoice is Null And @parmInvoice='&lt;Draft&gt;')) Group By AR.WBS1) AS AR On
billARDetail.MainWBS1 = AR.WBS1 
/****SubQuery to get invoice totals****/
 left  join  (SELECT   billInvSums.invoice as sumInvoice,  sum(billInvSums.finalAmount) AS finalAmount  
FROM billInvSums  
WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And ArrayType = 'I' 
AND (billInvSums.Invoice = @parmInvoice) 
AND (billInvSums.MainWBS1 = @parmMainWBS1 OR billInvSums.MainWBS1 = /*N*/'~')  
GROUP BY billInvSums.invoice) AS summary ON  (billARDetail.invoice = summary.sumInvoice)  
WHERE @PreInvoice = 'N' And (billARDetail.Invoice = @parmInvoice) 
AND (billARDetail.MainWBS1 = @parmMainWBS1 OR billARDetail.MainWBS1 = /*N*/'~') 
And (billARDetail.billwbs1 = /*N*/'~' Or (billGroup &lt;&gt; 'Y' And (billARDetail.billWBS2 = /*N*/' ' Or billARDetail.billWBS2 = /*N*/'~' Or printWBS1AR = 'Y')))  
AND @parmOnGroup = 'Y'
GROUP BY invoiceNumber, invoiceDate, retainageFormat, printAR.billGroup,printAR.printWBS1AR,summary.finalAmount  
 ORDER BY 1</CommandText>
