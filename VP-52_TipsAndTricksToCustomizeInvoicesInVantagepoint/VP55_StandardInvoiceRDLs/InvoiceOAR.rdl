  <Code>Public Function InvoiceLeadingZeros (ByVal DisplayInvoice As String) As String
	Dim i AS Integer
		i = 1
		Do While Mid(DisplayInvoice, i, 1) = "0"
			i = i + 1
		Loop
		DisplayInvoice = Mid(DisplayInvoice, i)
	Return DisplayInvoice
End Function</Code>
