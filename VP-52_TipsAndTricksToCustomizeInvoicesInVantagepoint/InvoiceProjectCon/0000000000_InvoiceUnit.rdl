    <DataSet Name="UnitDataset">
      <Query>
        <DataSourceName>VisionDemo50__CAMDB7_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@parmInvoice">
            <Value>=Parameters!parmInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DecimalPlaces">
            <Value>=Parameters!DecimalPlaces.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmMainWBS1">
            <Value>=Parameters!parmMainWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PreInvoice">
            <Value>=Parameters!PreInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BillingCurrencyCode">
            <Value>=Parameters!BillingCurrencyCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS1">
            <Value>=Parameters!parmWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS2">
            <Value>=Parameters!parmWBS2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS3">
            <Value>=Parameters!parmWBS3.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
unitName,
transDate,
Case When billUnitDetail.dispFormat = '2' Then unitName Else '' End As unitGroup, 
CASE WHEN billUnitDetail.dispFormat = '1' THEN billUnitDetail.unitName ELSE billUnitDetail.description END As description,
showMult,
billUnitDetail.showDate,
billUnitDetail.showCalc,
unitDetail,
dispFormat,
quantity,
multiplier,
rate,
calculation,
UN.singleLabel,
UN.pluralLabel,
CAST(@DecimalPlaces as int) As DecimalPlaces,
amount,
IsNull(summary.finalAmount, 0) As SectionTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
 FROM billUnitDetail inner join UN on billUnitDetail.UnitTable = UN.UnitTable AND billUnitDetail.Unit = UN.Unit
 left join 
/*SubQuery to get invoice totals*/
 (SELECT 
  billInvSums.invoice as sumInvoice,
  billInvSums.mainWBS1 as sumMainWBS1,
  billInvSums.BillWBS1 as sumBillWBS1,
  billInvSums.BillWBS2 as sumBillWBS2,
  billInvSums.BillWBS3 as sumBillWBS3, 
  sum(billInvSums.finalAmount) AS finalAmount
  FROM billInvSums 
  WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And 
ArrayType = 'I' AND Section = /*N*/'U' AND
 (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 AND (BillWBS1 = @parmWBS1 OR BillWBS1 = /*N*/'~') 
 AND (BillWBS2 = @parmWBS2 OR BillWBS2 = /*N*/'~') 
 AND (BillWBS3 = @parmWBS3 OR BillWBS3 = /*N*/'~') 
 GROUP BY billInvSums.invoice, billInvSums.mainWBS1, 
 billInvSums.BillWBS1, billInvSums.BillWBS2, billInvSums.BillWBS3) AS summary ON 
 (billUnitDetail.invoice = summary.sumInvoice) AND 
 (billUnitDetail.mainWBS1 = summary.sumMainWBS1) AND 
 (billUnitDetail.BillWBS1 = summary.sumBillWBS1) AND 
 (billUnitDetail.BillWBS2 = summary.sumBillWBS2) AND 
 (billUnitDetail.BillWBS3 = summary.sumBillWBS3)
 WHERE @PreInvoice = 'N' And Amount &lt;&gt; 0 AND IsNull(RateFlag, '') &lt;&gt; 'N'
          AND (billUnitDetail.Invoice = @parmInvoice)
          AND (billUnitDetail.MainWBS1 = @parmMainWBS1 OR billUnitDetail.MainWBS1 = /*N*/'~')
          AND (billUnitDetail.BillWBS1 = @parmWBS1 OR billUnitDetail.BillWBS1 = /*N*/'~')
          AND (billUnitDetail.BillWBS2 = @parmWBS2 OR billUnitDetail.BillWBS2 = /*N*/'~')
          AND (billUnitDetail.BillWBS3 = @parmWBS3 OR billUnitDetail.BillWBS3 = /*N*/'~')
          ORDER BY 3, 2, 4</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
