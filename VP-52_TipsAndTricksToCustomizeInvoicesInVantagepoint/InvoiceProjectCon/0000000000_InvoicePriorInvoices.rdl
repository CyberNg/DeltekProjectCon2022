    <DataSet Name="PriorInvoicesDataset">
      <Query>
        <DataSourceName>VisionDemo51__camdb7_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@parmInvoice">
            <Value>=Parameters!parmInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PreInvoice">
            <Value>=Parameters!PreInvoice.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BillingCurrencyCode">
            <Value>=Parameters!BillingCurrencyCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmMainWBS1">
            <Value>=Parameters!parmMainWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@feeJTDFormat">
            <Value>=Parameters!feeJTDFormat.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@priorInvSubtotal">
            <Value>=Parameters!priorInvSubtotal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmWBS1">
            <Value>=Parameters!parmWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmOnWBS1">
            <Value>=Parameters!parmOnWBS1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@parmOnGroup">
            <Value>=Parameters!parmOnGroup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/**********************************WBS1**************************************/
SELECT billPriorInvoices.InvoiceNumber As Invoice,  
billPriorInvoices.InvoiceDate As InvoiceDate,
sum(billPriorInvoices.PriorAmt) As PriorAmt,  
sum(billPriorInvoices.OtherAmt) As OtherAmt, 
sum(IsNull(summary.finalSubtotal, 0)+IsNull(summaryFee.finalSubtotal, 0)+IsNull(summaryAdd.finalSubtotal, 0)) as subtotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM billPriorInvoices 

left join
/*SubQuery to get subtotals*/
 (SELECT 
  billInvSums.invoice as sumInvoice,
  billInvSums.mainWBS1 as sumMainWBS1,
  billInvSums.BillWBS1 as sumBillWBS1,
  sum(billInvSums.finalAmount) AS finalSubtotal
  FROM billInvSums 
  WHERE @feeJTDFormat = 'Y' And @priorInvSubtotal = 'Y' And 
ArrayType = 'I' AND PreInvoice = @PreInvoice and Section not in ('F', 'A', 'T','Ret','I')
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 AND (billInvSums.BillWBS1 = @parmWBS1 OR billInvSums.BillWBS1 = /*N*/'~') 
 GROUP BY billInvSums.invoice, billInvSums.mainWBS1, billInvSums.BillWBS1) AS summary ON 
 (billPriorInvoices.invoice = summary.sumInvoice) AND 
 (billPriorInvoices.mainWBS1 = summary.sumMainWBS1) AND
 (billPriorInvoices.BillWBS1 = summary.sumBillWBS1)

LEFT JOIN
/*SubQuery to get fee subtotals*/
 (SELECT 
  billFeeDetail.invoice as sumInvoice,
  billFeeDetail.mainWBS1 as sumMainWBS1,
  billFeeDetail.BillWBS1 as sumBillWBS1,
  Min(billFeeDetail.EarnedFee) AS finalSubtotal
  FROM billFeeDetail
  WHERE @feeJTDFormat = 'Y' And @priorInvSubtotal = 'Y' And 
 PreInvoice = @PreInvoice 
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 AND (billFeeDetail.BillWBS1 = @parmWBS1 OR billFeeDetail.BillWBS1 = /*N*/'~') 
 GROUP BY billFeeDetail.invoice, billFeeDetail.mainWBS1, billFeeDetail.BillWBS1) AS summaryFee ON 
 (billPriorInvoices.invoice = summaryFee.sumInvoice) AND 
 (billPriorInvoices.mainWBS1 = summaryFee.sumMainWBS1) AND
 (billPriorInvoices.BillWBS1 = summaryFee.sumBillWBS1)

LEFT JOIN
/*SubQuery to get fee subtotals*/
 (SELECT 
  billAddDetail.invoice as sumInvoice,
  billAddDetail.mainWBS1 as sumMainWBS1,
  billAddDetail.BillWBS1 as sumBillWBS1,
  Sum(billAddDetail.Amount) AS finalSubtotal
  FROM billAddDetail
  WHERE @feeJTDFormat = 'Y' And @priorInvSubtotal = 'Y'
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 AND (billAddDetail.BillWBS1 = @parmWBS1 OR billAddDetail.BillWBS1 = /*N*/'~') 
 GROUP BY billAddDetail.invoice, billAddDetail.mainWBS1, billAddDetail.BillWBS1) AS summaryAdd ON 
 (billPriorInvoices.invoice = summaryAdd.sumInvoice) AND 
 (billPriorInvoices.mainWBS1 = summaryAdd.sumMainWBS1) AND
 (billPriorInvoices.BillWBS1 = summaryAdd.sumBillWBS1)

Where @feeJTDFormat = 'Y' And (billPriorInvoices.Invoice = @parmInvoice) And (billPriorInvoices.MainWBS1 = @parmMainWBS1 OR billPriorInvoices.MainWBS1 = /*N*/'~') 
AND (billPriorInvoices.BillWBS1 = @parmWBS1 OR billPriorInvoices.BillWBS1 = /*N*/'~')  
AND billPriorInvoices.Billwbs1 &lt;&gt; /*N*/'~' And (billPriorInvoices.billWBS2 = /*N*/' ' Or billPriorInvoices.billWBS2 = /*N*/'~') AND @parmOnWBS1 = 'Y'
GROUP BY 
billPriorInvoices.InvoiceNumber, billPriorInvoices.InvoiceDate


/*******************************GROUP ***************************************/
UNION ALL SELECT billPriorInvoices.InvoiceNumber As Invoice,  
billPriorInvoices.InvoiceDate As InvoiceDate,
sum(billPriorInvoices.PriorAmt) As PriorAmt,  
sum(billPriorInvoices.OtherAmt) As OtherAmt, 
sum(IsNull(summary.finalSubtotal, 0)+IsNull(summaryFee.finalSubtotal, 0)+IsNull(summaryAdd.finalSubtotal, 0)) as subtotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode
FROM billPriorInvoices 
left join (
Select billInvMaster.invoice as invInvoice,billGroup
From billInvMaster  
WHERE @PreInvoice = 'N' And (billInvMaster.Invoice = @parmInvoice) 
AND (billInvMaster.MainWBS1 = @parmMainWBS1 OR billInvMaster.MainWBS1 = /*N*/'~') 
and MainType &lt;&gt; 'S' And @feeJTDFormat = 'Y'
Group By billInvMaster.invoice, billGroup) As printBTD On (billPriorInvoices.invoice = printBTD.invInvoice) 

left join
/*SubQuery to get subtotals*/
 (SELECT 
  billInvSums.invoice as sumInvoice,
  billInvSums.mainWBS1 as sumMainWBS1,
  sum(billInvSums.finalAmount) AS finalSubtotal
  FROM billInvSums 
  WHERE @feeJTDFormat = 'Y' And @priorInvSubtotal = 'Y' And 
ArrayType = 'I' AND PreInvoice = @PreInvoice and Section not in ('F','A','T','Ret','I')
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 GROUP BY billInvSums.invoice, billInvSums.mainWBS1) AS summary ON 
 (billPriorInvoices.invoice = summary.sumInvoice) AND 
 (billPriorInvoices.mainWBS1 = summary.sumMainWBS1)

LEFT JOIN
/*SubQuery to get fee subtotals*/
 (SELECT 
  billFeeDetail.invoice as sumInvoice,
  billFeeDetail.mainWBS1 as sumMainWBS1,
  Min(billFeeDetail.EarnedFee) AS finalSubtotal
  FROM billFeeDetail
  WHERE @feeJTDFormat = 'Y' And @priorInvSubtotal = 'Y' And 
 PreInvoice = @PreInvoice 
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 GROUP BY billFeeDetail.invoice, billFeeDetail.mainWBS1) AS summaryFee ON 
 (billPriorInvoices.invoice = summaryFee.sumInvoice) AND 
 (billPriorInvoices.mainWBS1 = summaryFee.sumMainWBS1) 

LEFT JOIN
/*SubQuery to get fee subtotals*/
 (SELECT 
  billAddDetail.invoice as sumInvoice,
  billAddDetail.mainWBS1 as sumMainWBS1,
  Sum(billAddDetail.Amount) AS finalSubtotal
  FROM billAddDetail
  WHERE @feeJTDFormat = 'Y' And @priorInvSubtotal = 'Y'
 AND (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 GROUP BY billAddDetail.invoice, billAddDetail.mainWBS1) AS summaryAdd ON 
 (billPriorInvoices.invoice = summaryAdd.sumInvoice) AND 
 (billPriorInvoices.mainWBS1 = summaryAdd.sumMainWBS1) 

Where @feeJTDFormat = 'Y' And (billPriorInvoices.Invoice = @parmInvoice) And (billPriorInvoices.MainWBS1 = @parmMainWBS1 OR billPriorInvoices.MainWBS1 = /*N*/'~') 
AND (billPriorInvoices.billwbs1 = /*N*/'~' Or (billGroup &lt;&gt; 'Y' And (billPriorInvoices.billWBS2 = /*N*/' ' Or billPriorInvoices.billWBS2 = /*N*/'~'))) AND @parmOnGroup = 'Y'
GROUP BY 
billPriorInvoices.InvoiceNumber, billPriorInvoices.InvoiceDate


ORDER BY 1, 2</CommandText>
