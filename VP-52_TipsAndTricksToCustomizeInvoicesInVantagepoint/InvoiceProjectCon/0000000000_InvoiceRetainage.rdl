 And (billRetDetail.billWBS1 = /*N*/'~' And billRetDetail.billWBS2 = /*N*/'~' And billRetDetail.billWBS3 = /*N*/'~')
 And @parmOnGroup = 'Y'

 ORDER BY 1</CommandText>
