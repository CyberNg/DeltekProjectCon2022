        <CommandText>/******************************************************************** WBS3 ******************************************/
SELECT
prior,
limit,
pct/100 AS pct,
amount,
retainage,
IsNull(summary.finalAmount, 0) As SectionTotal,
CAST(@BillingCurrencyCode as varchar) As CurrencyCode,
CAST(@DecimalPlaces as int) As DecimalPlaces
 FROM billRetDetail
 left join
/*SubQuery to get invoice totals*/
 (SELECT 
  billInvSums.invoice as sumInvoice,
  billInvSums.mainWBS1 as sumMainWBS1,
  billInvSums.BillWBS1 as sumBillWBS1, 
  billInvSums.BillWBS2 as sumBillWBS2,
  billInvSums.BillWBS3 as sumBillWBS3,
  sum(billInvSums.finalAmount) AS finalAmount
  FROM billInvSums 
  WHERE @PreInvoice = 'N' And billInvSums.PreInvoice = 'N' And 
 ArrayType = 'I' AND Section = /*N*/'Ret' AND
 (Invoice = @parmInvoice) 
 AND (MainWBS1 = @parmMainWBS1 OR MainWBS1 = /*N*/'~') 
 AND (BillWBS1 = @parmWBS1 OR BillWBS1 = /*N*/'~') 
 AND (BillWBS2 = @parmWBS2 OR BillWBS2 = /*N*/'~') 
 AND (BillWBS3 = @parmWBS3 OR BillWBS3 = /*N*/'~') 
 GROUP BY billInvSums.invoice, billInvSums.mainWBS1,
 billInvSums.BillWBS1, billInvSums.BillWBS2, billInvSums.BillWBS3) AS summary ON 
 (billRetDetail.invoice = summary.sumInvoice) AND 
 (billRetDetail.mainWBS1 = summary.sumMainWBS1) AND 
 (billRetDetail.BillWBS1 = summary.sumBillWBS1) AND
 (billRetDetail.BillWBS2 = summary.sumBillWBS2) AND
 (billRetDetail.BillWBS3 = summary.sumBillWBS3)
 WHERE @PreInvoice = 'N' And (billRetDetail.Invoice = @parmInvoice) 
 AND (billRetDetail.MainWBS1 = @parmMainWBS1 OR billRetDetail.MainWBS1 = /*N*/'~') 
 AND (billRetDetail.BillWBS1 = @parmWBS1 OR billRetDetail.BillWBS1 = /*N*/'~') 
 AND (billRetDetail.BillWBS2 = @parmWBS2 OR billRetDetail.BillWBS2 = /*N*/'~') 
 AND (billRetDetail.BillWBS3 = @parmWBS3 OR billRetDetail.BillWBS3 = /*N*/'~') 
 And billRetDetail.billWBS3 &lt;&gt; /*N*/'~' And billRetDetail.billWBS3 &lt;&gt; /*N*/' ' 
 And @parmOnWBS3 = 'Y'

/******************************************************************** WBS2 *****************************************/

