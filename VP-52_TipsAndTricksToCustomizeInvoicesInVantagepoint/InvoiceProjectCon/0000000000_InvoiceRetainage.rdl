 And billRetDetail.billWBS1 &lt;&gt; /*N*/'~' And ((billRetDetail.billWBS2 = /*N*/'~' And billRetDetail.BillWBS3 = /*N*/'~') Or (billRetDetail.billWBS2 = /*N*/' ' And billRetDetail.billWBS3 = /*N*/' '))
