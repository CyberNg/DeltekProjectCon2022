And billRetDetail.billWBS2 &lt;&gt; /*N*/'~' And billRetDetail.billWBS2 &lt;&gt; /*N*/' ' And (billRetDetail.billWBS3 = /*N*/'~' Or billRetDetail.billWBS3 = /*N*/' ')
 And @parmOnWBS2 = 'Y'

/******************************************************************** WBS1 *******************************************/

UNION ALL SELECT
