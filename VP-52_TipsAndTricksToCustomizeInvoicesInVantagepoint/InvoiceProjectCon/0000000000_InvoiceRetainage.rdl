 GROUP BY billInvSums.invoice, billInvSums.mainWBS1) AS summary ON 
 (billRetDetail.invoice = summary.sumInvoice) AND 
 (billRetDetail.mainWBS1 = summary.sumMainWBS1)
 WHERE @PreInvoice = 'N' And (billRetDetail.Invoice = @parmInvoice) 
